package de.jan.commandline.main;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		String input;
		boolean next = true;
		do {
			input = JOptionPane.showInputDialog(null, "Command:", "Command Line", JOptionPane.DEFAULT_OPTION);
			switch(input) {
			case "/exit":
				next = false;
			break;
			}
		}while(next);

	}

}
